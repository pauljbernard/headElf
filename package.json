{
  "name": "headelf-executive-intelligence",
  "version": "1.0.0",
  "description": "Comprehensive C-Suite Executive Intelligence System for Claude Code",
  "main": "src/core/executive-intelligence-engine.ts",

  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "validate": "npm run lint && npm run build && npm run test",
    "install:claude-code": "node scripts/install-claude-code.js",
    "setup:complete": "npm install && npm run build && npm run install:claude-code"
  },

  "keywords": [
    "claude-code",
    "executive-intelligence",
    "c-suite",
    "cto", "cio", "ciso", "cfo", "coo", "clo", "chro", "cmso", "cpo",
    "business-intelligence",
    "enterprise-architecture",
    "decision-making",
    "artificial-intelligence",
    "typescript",
    "venture-capital",
    "mergers-acquisitions",
    "digital-transformation",
    "risk-management"
  ],

  "author": "HeadElf Executive Intelligence System",
  "license": "MIT",
  "homepage": "https://pauljbernard.github.io/headElf",
  "repository": {
    "type": "git",
    "url": "https://github.com/pauljbernard/headElf.git"
  },
  "bugs": {
    "url": "https://github.com/pauljbernard/headElf/issues"
  },

  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },

  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.0"
  },

  "dependencies": {
    "yaml": "^2.3.0",
    "uuid": "^9.0.0",
    "lodash": "^4.17.21",
    "@types/lodash": "^4.14.195",
    "@types/uuid": "^9.0.2"
  },

  "peerDependencies": {
    "claude-code": ">=2.0.0"
  },

  "files": [
    "src/**/*",
    "skills/**/*",
    "scripts/**/*",
    "dist/**/*",
    "headelf.yml",
    "README.md",
    "LICENSE"
  ],

  "directories": {
    "lib": "src",
    "test": "tests",
    "doc": "docs"
  },

  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },

  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/pauljbernard"
  }
}